{
  "name": "underscores",
  "version": "1.0.0",
  "description": "Hi. I'm a starter theme called _s, or underscores, if you like. I'm a theme meant for hacking so don't use me as a Parent Theme. Instead try turning me into the next, most awesome, WordPress theme out there. That's what I'm here for.",
  "author": "Automattic Theme Team",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/Automattic/_s#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/_s.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/_s/issues"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.14",
    "@wordpress/scripts": "^19.2.2",
    "autoprefixer": "^10.4.20",
    "dir-archiver": "^1.1.1",
    "postcss": "^8.5.2",
    "rtlcss": "^3.5.0",
    "sass": "^1.69.7"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "build:css": "npx @tailwindcss/cli@latest -i ./src/input.css -o ./style.css --minify",
    "watch:css": "npx @tailwindcss/cli@latest -i ./src/input.css -o ./style.css --watch",
    "switch-theme": "php -r \"define('ABSPATH', true); function get_template_directory() { return getcwd(); } include 'inc/theme-switcher.php'; echo digitalia_force_rebuild_input_css();\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@tailwindcss/typography": "^0.5.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.475.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0"
  }
}
